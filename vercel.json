{
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/trigger-content-generation",
      "schedule": "30 9 * * *"
    },
    {
      "path": "/api/trigger-daily-emails", 
      "schedule": "0 10 * * *"
    }
  ]
}